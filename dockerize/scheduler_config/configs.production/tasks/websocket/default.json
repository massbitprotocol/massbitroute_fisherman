{
  "name": "Websocket",
  "active": true,
  "url_template": "{{ws_scheme}}://{{provider.ip}}/",
  "phases": ["*"],
  "provider_types": ["node"],
  "blockchains": ["*"],
  "networks": ["*"],
  "request_timeout": 3000,
  "headers": {},
  "repeat_number": 0,
  "interval": 120000,
  "headers": {
    "X-Api-Key": "{{provider.appKey}}",
    "Host": "ws-{{provider.id}}.{{provider.type}}.mbr.{{domain}}"
  },
  "body": "",
  "response" : {
    "response_type" : "text"
  },
  "assignment": {
    "broadcast": false,
    "worker_number": 1,
    "nearby_only": false,
    "by_distance": false
  },
  "thresholds": {

  }
}