{
  "default": {
    "benchmark_thread": 5,
    "benchmark_connection": 5,
    "benchmark_duration": 15000,
    "benchmark_rate": 10,
    "script": "massbit.lua",
    "histograms": [90,95,99,100],
    "http_method": "POST",
    "url_template":"http://{{provider.ip}}/",
    "judge_histogram_percentile":  95,
    "response_threshold": 500,
    "headers": {
      "content-type":"application/json",
      "X-Api-Key": "{{provider.appKey}}",
      "Host": "{{provider.id}}.{{provider.type}}.mbr.{{domain}}"
    },
    "body": {
      "jsonrpc": "2.0",
      "method": "eth_getBlockByNumber",
      "params": ["latest",true],
      "id": 1
    },
    "assignment": {
      "broadcast": false,
      "worker_number": 10,
      "nearby_only": false,
      "by_distance": true
    }
  },
  "verification": {
    "dependencies": {
      "HttpRequest": [
        "RoundTripTime"
      ]
    }
  },
  "regular": {
    "benchmark_thread": 5,
    "benchmark_connection": 5,
    "benchmark_duration": 15000,
    "benchmark_rate": 10,
    "script": "massbit.lua",
    "histograms": [90,95,99,100],
    "url_template":"http://{{provider.ip}}/",
    "judge_histogram_percentile":  90,
    "response_threshold": 500
  }
}